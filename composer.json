{
    "name": "fragale/lgenerators",
    "description": "Laravel 5 Generators, basado en la version 1.1 de J.Way",
    "keywords": ["laravel", "generators", "artisan"],
    "license": "MIT",    
    "authors": [
        {
            "name": "Maximiliano fragale",
            "email": "fragale@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "mustache/mustache": "2.4.*",
        "twbs/bootstrap": "dev-master",         
        "laravelcollective/html": "~5.0"
    },
    "require-dev": {
        "mockery/mockery": "0.8.*",
        "illuminate/support": "~4",
        "illuminate/console": "~4",
        "illuminate/filesystem": "~4"
    },
    "autoload": {
        "psr-4": {
            "Fragale\\": "src/Fragale/"
        },        
        "classmap": [
            "src/Fragale/Generators/Generators",
            "src/Fragale/Helpers"
        ]
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
