{
    "name": "fragale/l4generators",
    "description": "Laravel 4 Generators, basado en la version 1.1 de J.Way",
    "keywords": ["laravel", "generators", "artisan"],
    "authors": [
        {
            "name": "Maximiliano fragale",
            "email": "fragale@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "mustache/mustache": "2.4.*"
    },
    "require-dev": {
        "mockery/mockery": "0.8.*",
        "illuminate/support": "~4",
        "illuminate/console": "~4",
        "illuminate/filesystem": "~4"
    },
    "autoload": {
        "psr-0": {
            "Fragale\\Generators": "src/"
        },
        "classmap": [
            "src/Fragale/Generators/Generators"
        ]
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
